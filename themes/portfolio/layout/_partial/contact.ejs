<section id="contact">
    <div class="container">
        <div class="row">
            <div class="col-lg-8 col-lg-offset-2 text-center">
                <h2 class="section-heading">Let's Get In Touch!</h2>
                <hr class="primary">
                <p>Interested in working together? Send an Email below.</p>
            </div>
            <div class="col-lg-12 text-center">
                <% if(theme.emailAddress){%>
                    <i class="fa fa-envelope-o fa-3x sr-contact"></i>
                    <p>
                        <a href="mailto:<%= theme.emailAddress %>" target="_blank"><%= theme.emailAddress %></a>
                    </p>
                <%}%>
            </div>
        </div>
    </div>
</section>
